programa
{
	
//Lista de exercícios -3
	funcao inicio()
	{
		//Declarando variáveis
		inteiro A,B,C

		//Pedindo valores
		escreva("Digite o valor de A: ")
		leia(A)
		escreva("\nDigite o valor de B: ")
		leia(B)
		
		//Calculando
		se(A==B){
			C = A + B
			escreva("Resultado: ",C)
		}senao{
			C = A * B
			escreva("Resultado: ",C)
		}


			
	}
}
